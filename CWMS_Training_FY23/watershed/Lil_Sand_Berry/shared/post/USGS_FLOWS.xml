<?xml version="1.0" encoding="UTF-8"?>
<dataexchange-configuration xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.hec.usace.army.mil/xmlSchema/cwms/dataexchangeconfiguration.xsd">
  <datastore>
    <model-plugin id="forecast-model-plugin" />
  </datastore>
  <datastore>
    <dssfile id="DSS">
      <filepath>ControlGages/LSA_Controls.dss</filepath>
    </dssfile>
  </datastore>
  <datastore>
    <dssfile id="forecast-dss" />
  </datastore>
  <ts-dataexchange-set id="USGS FLOWS" exchange-data="false" sort-order="5.0">
    <datastore-ref id="DSS" dss-regular-ts-store-rule="Replace All" dss-irregular-ts-store-rule="Merge" ts-override-protection="ON" />
    <datastore-ref id="forecast-model-plugin" />
    <ts-mapping-set />
  </ts-dataexchange-set>
  <ts-dataexchange-set id="USGS FLOWS1" exchange-data="false" sort-order="5.0">
    <datastore-ref id="DSS" dss-regular-ts-store-rule="Replace All" dss-irregular-ts-store-rule="Merge" ts-override-protection="ON" />
    <datastore-ref id="forecast-dss" />
    <ts-mapping-set>
      <ts-mapping>
        <dss-timeseries datastore-id="forecast-dss">//GRAYSONKY/FLOW//1HOUR/OBS/</dss-timeseries>
        <dss-timeseries datastore-id="DSS">//GRAYSONKY/FLOW//1HOUR/OBS/</dss-timeseries>
      </ts-mapping>
    </ts-mapping-set>
  </ts-dataexchange-set>
  <timewindow>
    <starttime>EXTRACT+PT0M</starttime>
    <endtime>FORECAST+PT0M</endtime>
  </timewindow>
</dataexchange-configuration>

