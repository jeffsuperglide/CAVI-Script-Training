<?xml version="1.0" encoding="UTF-8"?>
<dataexchange-configuration xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.hec.usace.army.mil/xmlSchema/cwms/dataexchangeconfiguration.xsd">
  <datastore>
    <model-plugin id="forecast-model-plugin" />
  </datastore>
  <datastore>
    <dssfile id="DSS">
      <filepath>../../database/data.dss</filepath>
    </dssfile>
  </datastore>
  <datastore>
    <dssfile id="forecast-dss" />
  </datastore>
  <ts-dataexchange-set id="15MinElev" exchange-data="true" sort-order="2.0">
    <datastore-ref id="DSS" />
    <datastore-ref id="forecast-model-plugin" dss-regular-ts-store-rule="Replace All" dss-irregular-ts-store-rule="Merge" ts-override-protection="OFF" />
    <ts-mapping-set />
  </ts-dataexchange-set>
  <ts-dataexchange-set id="15MinElev1" exchange-data="true" sort-order="2.0">
    <datastore-ref id="DSS" />
    <datastore-ref id="forecast-dss" dss-regular-ts-store-rule="Replace All" dss-irregular-ts-store-rule="Merge" ts-override-protection="OFF" />
    <ts-mapping-set>
      <ts-mapping>
        <dss-timeseries datastore-id="DSS">//GRAYSON-LAKE/ELEV/11Oct2022 - 15Oct2022/15Minute/OBS/</dss-timeseries>
        <dss-timeseries datastore-id="forecast-dss">//GRAYSON-LAKE/ELEV/11Oct2022 - 15Oct2022/1HOUR/OBS/</dss-timeseries>
      </ts-mapping>
    </ts-mapping-set>
  </ts-dataexchange-set>
  <timewindow>
    <starttime>EXTRACT-PT2H</starttime>
    <endtime>FORECAST+PT2H</endtime>
  </timewindow>
</dataexchange-configuration>

